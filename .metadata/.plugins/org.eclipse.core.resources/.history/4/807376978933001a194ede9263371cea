package guimodule;

import processing.core.PApplet;

public class MyDisplay extends PApplet{
	
	public void setup()
	{
		
		// creates canvas size first
		
		size(400, 400);
		background(200,200, 200);
		
		// making happy face shape using processing library
		
		
		
		
	}
	
	public void draw()
	{
		// making happy face shape using processing library inside the canvas
		ellipse (200, 200, 390, 390);
		
	}

}
